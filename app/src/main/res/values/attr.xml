<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="editTextHint" format="string"/>
        <attr name="inputType" format="enum">
            <enum name="DEFAULT" value="0"/>
            <enum name="NAME" value="1"/>
            <enum name="PASSWORD" value="2"/>
            <enum name="EMAIL" value="3"/>
        </attr>
        <attr name="typeEditText" format="string"/>
    </declare-styleable>

    <declare-styleable name="ButtonMenu">
        <attr name="android:entries" />
        <attr name="text" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomTable">
        <attr name="title" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomRow">
        <attr name="key1" format="string"/>
        <attr name="key2" format="string"/>
    </declare-styleable>
</resources>